public class BankAccount {

    String accName;
    String password;
    int pin;
    int accNum;
    double accBal;

    public BankAccount(){
	String accName = "no name input";
	String password = "no password input";
	int pin = 0000;
	int accNum = 000000000;
	double accBal = 0.0 ;
    }
	
    public void setAccPass(String name, String pass) {
	accName = name;
	password = pass;
	System.out.println("SetNamePass successful.");
    }

    public void setPinNum(int newpin, int newAccNum) {
	pin = newpin;
	accNum = newAccNum;
	System.out.println("SetPinNum successful.");
    }
    
    
    public void printInfo() {
        
 	System.out.println("Account Name: " + accName);
        System.out.println("Account Password: " + password);
	System.out.println("PIN: " + pin);
	System.out.println("Account#: " + accNum);
	System.out.println("Account Balance: $" + accBal);
	System.out.println(" ");
    } 

    public void deposit(double cashIn){
	double newBal;

	newBal = (accBal + cashIn);
	accBal = newBal;
	System.out.println("Deposited $"
			   + cashIn
			   +". Current Balance: $"
			   + accBal);
    }

    public void withdraw(double cashOut){
	double newBal;

	newBal = (accBal - cashOut);
	accBal = newBal;
	System.out.println("Withdrew $"
			   + cashOut
			   +". Current Balance: $"
			   + accBal);
    }

    public static void main(String[] args){
	
	BankAccount grace = new BankAccount();
	grace.setAccPass("Grace","12345");
	grace.setPinNum(2000,00001);
	
        grace.printInfo();
	
	grace.deposit(30);
	grace.withdraw(10);
    }
}



